<% if @guess > @secret_num %>
	<p>You're too high</p>
	<p><%= puts "#{@secret_num}" %></p>
	<%= image_tag "games/loser.gif" %>
<% elsif @guess < @secret_num %>
	<p>You're too low</p>
	<p><%= puts "#{@secret_num}" %></p>
	<%= image_tag "games/loser.gif" %>
<% else %>
	<h1>Winner!</h1>
	<p><%= puts "#{@secret_num}" %></p>
	<%= image_tag "games/winner.gif" %>
<% end %>